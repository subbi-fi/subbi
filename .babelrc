{
  "presets": [
    "@babel/preset-env",
    ["@babel/preset-typescript",
      {
        "isTsx": true
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
          "root": [
          "./src"
        ]
      }
    ],
    "inline-react-svg",
    [
      "babel-plugin-styled-components",
      {
        "ssr": true 
      }
    ]
  ]
}